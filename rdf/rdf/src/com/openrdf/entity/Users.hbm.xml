<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.openrdf.entity.Users" table="users">
		<id name="userId" column="userId">
			<generator class="uuid"></generator>
		</id>
	
	</class>


	<class name="com.giant.zzidc.entity.RcNetwork" table="rc_network"
		catalog="zzidc_db">
		<id name="networkid" type="java.lang.Long">
			<column name="networkid" />
			<generator class="identity" />
		</id>
		<property name="vsid" type="java.lang.Long">
			<column name="vsid">
				<comment>虚拟交换机id</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
